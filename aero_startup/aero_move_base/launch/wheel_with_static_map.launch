<?xml version="1.0"?>
<launch>
  <arg name="RUN_BRINGUP" default="true" />

  <!--- Run bringup -->
  <include if="$(arg RUN_BRINGUP)"
           file="$(find aero_startup)/aero_move_base/launch/wheel_bringup.launch" />

  <!-- Run map navigation -->
  <arg name="map_file" default="$(find aero_startup)/aero_move_base/maps/room83a3_cost.yaml"/>
  <arg name="costmap_file" default="$(find aero_startup)/aero_move_base/maps/room83a3_cost.yaml"/>
  <include file="$(find aero_startup)/aero_move_base/launch/static_map_navigation.launch">
    <arg name="map_file" value="$(arg map_file)" />
    <arg name="costmap_file" value="$(arg costmap_file)"/>
  </include>

</launch>
